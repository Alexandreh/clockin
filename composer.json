{
    "name": "alexandreh/clockin",
    "description": "Working time tracker",
    "keywords": ["time", "tracker", "work"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/lumen-framework": "5.1.*",
        "vlucas/phpdotenv": "~1.0",
        "baao/clear-view-cache": "^1.0",
        "barryvdh/laravel-debugbar": "^2.0",
        "neomerx/limoncello": "^0.5.0",
        "neomerx/cors-illuminate": "^0.2.0",
        "firebase/php-jwt": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "fzaninotto/faker": "~1.0",
        "laravel/homestead": "^2.1",
        "mockery/mockery": "^0.9.4",
        "symfony/finder": "^2.7",
        "symfony/css-selector": "^2.7"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
